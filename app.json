{
  "expo": {
    "name": "APPlication",
    "slug": "APPlication",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#4b0082"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#4b0082"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "description": "A personal resum√© built as a mobile app using React Native",
    "githubUrl": "https://github.com/wltrallen2/APPlication"
  }
}
